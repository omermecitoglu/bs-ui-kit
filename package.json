{
  "name": "@omer-x/bs-ui-kit",
  "version": "0.1.2",
  "description": "Bootstrap UI Kit for React.js",
  "keywords": [
    "react",
    "bootstrap",
    "ui-kit",
    "font-awesome"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omermecitoglu/bs-ui-kit.git"
  },
  "bugs": {
    "url": "https://github.com/omermecitoglu/bs-ui-kit/issues"
  },
  "homepage": "https://github.com/omermecitoglu/bs-ui-kit#readme",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Omer Mecitoglu",
    "email": "omer.mecitoglu@gmail.com",
    "url": "https://omermecitoglu.github.io"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./ActionButton": {
      "import": "./dist/components/ActionButton.js",
      "types": "./dist/components/ActionButton.d.ts"
    },
    "./CreateButton": {
      "import": "./dist/components/CreateButton.js",
      "types": "./dist/components/CreateButton.d.ts"
    },
    "./LinkButton": {
      "import": "./dist/components/LinkButton.js",
      "types": "./dist/components/LinkButton.d.ts"
    },
    "./form": {
      "import": "./dist/components/form/index.js",
      "types": "./dist/components/form.index.d.ts"
    },
    "./form/FormWithState": {
      "import": "./dist/components/form/FormWithState.js",
      "types": "./dist/components/form/FormWithState.d.ts"
    },
    "./form/Group": {
      "import": "./dist/components/form/Group.js",
      "types": "./dist/components/form/Group.d.ts"
    },
    "./form/Input": {
      "import": "./dist/components/form/Input.js",
      "types": "./dist/components/form/Input.d.ts"
    },
    "./form/Label": {
      "import": "./dist/components/form/Label.js",
      "types": "./dist/components/form/Label.d.ts"
    },
    "./form/ModalForm": {
      "import": "./dist/components/form/ModalForm.js",
      "types": "./dist/components/form/ModalForm.d.ts"
    },
    "./form/Select": {
      "import": "./dist/components/form/Select.js",
      "types": "./dist/components/form/Select.d.ts"
    },
    "./form/SubmitButton": {
      "import": "./dist/components/form/SubmitButton.js",
      "types": "./dist/components/form/SubmitButton.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc"
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "react-bootstrap": "^2.10.4"
  },
  "dependencies": {
    "classnames": "^2.5.1"
  },
  "devDependencies": {
    "@omer-x/eslint-config": "^1.0.7",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.3"
  }
}
